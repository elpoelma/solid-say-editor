<SaySolidContent @webId={{this.auth.webId}} @header="You have unsaved changes..."/>
<SaySolidFooter>
    {{#if this.auth.webId}}
    <WuButton @label="Save" @size="small" @isNarrow={{true}} @commandLocation="below" @onClick={{this.save}} />
