<SaySolidContent  @webId={{this.auth.webId}} @header="Your webID is: {{this.auth.webId}}"/>
<SaySolidFooter>
    {{#if this.auth.webId}}
        <WuButton @label="Close card" @size="small" @isNarrow={{true}} @commandLocation="below" @onClick={{this.close}} />
        {{else}}
        <WuButton @label="Login" @size="small" @isNarrow={{true}} @commandLocation="below" @onClick={{this.login}} />
        {{/if}}
</SaySolidFooter>