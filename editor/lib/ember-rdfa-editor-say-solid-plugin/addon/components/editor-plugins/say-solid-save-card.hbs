<div class="modal-dialog__content">
  <p class="u-spacer--tiny">
    {{#if this.auth.webId}}
    <strong>You have unsaved changes...</strong>
    {{else}}
    <strong>You are not logged in</strong>
    {{/if}}
  </p>
</div>
<div class="modal-dialog__footer">
 <WuButtonGroup>
    {{#if this.auth.webId}}
     {{#unless this.isEditMode}}
    <WuButton @label="Save" @size="small" @isNarrow={{true}} @commandLocation="below" @onClick={{this.save}} />
    {{/unless}}
    {{/if}}
  </WuButtonGroup>
</div>